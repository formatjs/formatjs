
FROM ubuntu:22.04

# Install dependencies (excluding tzdata)
RUN apt-get update && apt-get install -y build-essential make tar curl &&     rm -rf /var/lib/apt/lists/*

# Set working directory for downloading files
WORKDIR /tz

# Download tzdata and tzcode based on the version
RUN curl -o tzdata.tar.gz https://data.iana.org/time-zones/releases/tzdata2024b.tar.gz
RUN curl -o tzcode.tar.gz https://data.iana.org/time-zones/releases/tzcode2024b.tar.gz

# Verify the SHA-256 checksum
RUN echo "70e754db126a8d0db3d16d6b4cb5f7ec1e04d5f261255e4558a67fe92d39e550  tzdata.tar.gz" | sha256sum -c -
RUN echo "5e438fc449624906af16a18ff4573739f0cda9862e5ec28d3bcb19cbaed0f672  tzcode.tar.gz" | sha256sum -c -

# Extract the copied files into /tz and set up directories in a single layer
RUN tar -xzf tzdata.tar.gz -C /tz && \
    tar -xzf tzcode.tar.gz -C /tz && \
    mkdir -p /tzdir/usr/sbin /tzdir/usr/bin /tz_data_generated/zdump && \
    mkdir -p /tz_data_generated/zdump/Africa && \ 
mkdir -p /tz_data_generated/zdump/America && \ 
mkdir -p /tz_data_generated/zdump/America/Argentina && \ 
mkdir -p /tz_data_generated/zdump/America/Indiana && \ 
mkdir -p /tz_data_generated/zdump/America/Kentucky && \ 
mkdir -p /tz_data_generated/zdump/America/North_Dakota && \ 
mkdir -p /tz_data_generated/zdump/Antarctica && \ 
mkdir -p /tz_data_generated/zdump/Arctic && \ 
mkdir -p /tz_data_generated/zdump/Asia && \ 
mkdir -p /tz_data_generated/zdump/Atlantic && \ 
mkdir -p /tz_data_generated/zdump/Australia && \ 
mkdir -p /tz_data_generated/zdump/Europe && \ 
mkdir -p /tz_data_generated/zdump/Indian && \ 
mkdir -p /tz_data_generated/zdump/Pacific

# Pre-compile tzdata and run zdump commands
