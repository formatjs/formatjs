  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">= 18.x"
    }
  },
  "lint-staged": {
    "*": "bazel run //:format",
    "**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,astro,svelte}": "oxlint"
  },
  "packageManager": "pnpm@10.26.1",
  "pnpm": {
    "onlyBuiltDependencies": [],
    "packageExtensions": {
      "css-tree": {
        "dependencies": {
          "source-map-js": "*"
        }
      },
      "postcss": {
        "dependencies": {
          "source-map-js": "*"
        }
      },
      "@vue/compiler-core": {
        "dependencies": {
          "@babel/types": "*"
        }
      },
      "@vue/reactivity-transform": {
        "dependencies": {
          "@babel/types": "*"
        }
      },
      "@vue/test-utils": {
        "dependencies": {
          "@vue/compiler-dom": "*"
        }
      }
    },
    "patchedDependencies": {
      "make-plural-compiler@5.1.0": "patches/make-plural-compiler@5.1.0.patch",
      "@commitlint/rules": "patches/@commitlint__rules.patch",
      "@vue/compiler-sfc": "patches/@vue+compiler-sfc.patch",
      "@lerna-lite/version": "patches/@lerna-lite__version.patch"
    }
  },
  "repository": "formatjs/formatjs",
  "resolutions": {
    "@glimmer/interfaces": "0.94.6",
